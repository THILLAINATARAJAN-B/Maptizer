{
  "name": "locationiq-insights-backend",
  "version": "1.0.0",
  "description": "LocationIQ Insights Backend - API server for location analytics, comparisons, and AI-powered insights",
  "main": "server.js",
  "type": "commonjs",
  "author": "Your Team",
  "license": "MIT",
  "keywords": [
    "locationiq",
    "api",
    "backend",
    "express",
    "nodejs"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "serve": "npm start",
    "build": "echo 'No build step required for backend'",
    "test": "echo 'Tests not configured yet'",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "setup": "npm install && echo 'Backend setup complete! Create .env file and run npm run dev to start development server.'",
    "prod": "NODE_ENV=production node server.js",
    "debug": "node --inspect server.js",
    "logs": "pm2 logs locationiq-backend || echo 'PM2 not installed'",
    "monitor": "pm2 monit || echo 'PM2 not installed'"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "fs-extra": "^11.3.0",
    "groq-sdk": "^0.29.0",
    "helmet": "^7.2.0",
    "morgan": "^1.10.1",
    "pdfkit": "^0.17.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules",
      "logs"
    ],
    "ext": "js,json,env"
  },
  "repository": {
    "type": "git",
    "url": "your-repository-url"
  },
  "homepage": "your-repository-url#readme"
}
